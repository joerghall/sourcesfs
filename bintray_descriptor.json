{
  "package": {
    "name": "master",
    "repo": "sourcesfs",
    "subject": "joerghall",
    "vcs_url": "https://github.com/joerghall/sourcesfs.git",
    "licenses": ["MIT"]
  },

  "version": {
    "name": "${SOURCE_BRANCH}.${SOURCE_VERSION}",
    "desc": "This is a version",
    "released": "${BUILD_TIMESTAMP}",
    "gpgSign": false
  },

  "files":
  [
    {"includePattern": "build/([^-]+)-([^-]+)-([^-]+)-([^-]+)-([^-.]+)[.]([^.]+)\\.tar\\.gz", "uploadPattern": "$1-$2-$3-$4-$5.$6.$7.tgz"},
    {"includePattern": "build_centos/([^-]+)-([^-]+)-([^-]+)-([^-]+)-([^-.]+)[.]([^.]+)\\.rpm", "uploadPattern": "$1-$2-$3-$4-$5.$6.$7.rpm"},
    {"includePattern": "build_ubuntu/([^-]+)-([^-]+)-([^-]+)-([^-]+)-([^-.]+)[.]([^.]+)\\.dep", "uploadPattern": "$1-$2-$3-$4-$5.$6.$7.deb"}
  ],
  "publish": true
}
